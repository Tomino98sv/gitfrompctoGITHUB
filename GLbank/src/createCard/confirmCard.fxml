<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="createCard.ConfirmCard">
    <children>
        <VBox layoutX="14.0" prefHeight="175.0" prefWidth="274.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="95.0" prefWidth="274.0" text="Are you sure you want create New Card" textAlignment="CENTER">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Label>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#returnToCards" prefHeight="45.0" prefWidth="108.0" text="Cancel">
                            <font>
                                <Font size="21.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                            </HBox.margin>
                        </Button>
                        <Button layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#keepCreatingCard" prefHeight="45.0" prefWidth="108.0" text="Create">
                            <font>
                                <Font size="21.0" />
                            </font>
                            <HBox.margin>
                                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
