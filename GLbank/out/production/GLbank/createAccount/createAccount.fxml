<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="createAccount.CreateAccount">
   <children>
      <Label alignment="CENTER" layoutX="150.0" layoutY="20.0" prefHeight="46.0" prefWidth="300.0" text="Created New Account for: ">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox layoutX="100.0" layoutY="63.0" prefHeight="50.0" prefWidth="400.0">
         <children>
            <Label fx:id="fnameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="200.0" text="fname">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
            <Label fx:id="lnameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="45.0" prefWidth="200.0" text="lname">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
         </children></HBox>
      <Label fx:id="emailLabel" alignment="CENTER" layoutX="150.0" layoutY="100.0" prefHeight="46.0" prefWidth="300.0" text="email">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <VBox layoutX="26.0" layoutY="162.0" prefHeight="228.0" prefWidth="550.0">
         <children>
            <HBox prefHeight="24.0" prefWidth="550.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="200.0" text="AccountId:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <Label fx:id="accoundId" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="45.0" prefWidth="200.0" text="value">
                     <font>
                        <Font size="31.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="100.0" />
                     </HBox.margin>
                  </Label>
               </children></HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="24.0" prefWidth="550.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="200.0" text="AccountNumber:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <Label fx:id="accNumberLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="45.0" prefWidth="200.0" text="value">
                     <font>
                        <Font size="31.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="100.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="56.0" prefHeight="24.0" prefWidth="550.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="200.0" text="FirstDepot:">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <TextField fx:id="amountLabel" alignment="CENTER" onMouseClicked="#eraseWrongText" prefHeight="34.0" prefWidth="189.0" promptText="Mandatory">
                     <HBox.margin>
                        <Insets left="100.0" />
                     </HBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#confirmNewAccount" text="Confirm" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets left="230.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
